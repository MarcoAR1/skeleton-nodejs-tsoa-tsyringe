{
  "name": "skeleton-base",
  "version": "2.0.0",
  "description": "Base skeleton for next project in node",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/infrastructure/start",
    "build": "tsoa spec-and-routes && tsc && npm run copy:env",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "test": "jest -u --collect-coverage --colors",
    "copy:env": "cp .env.* dist",
    "lint": "eslint --fix --ext .ts ."
  },
  "dependencies": {
    "axios": "1.13.2",
    "body-parser": "2.2.1",
    "bodybuilder": "2.5.1",
    "bson": "7.0.0",
    "compression": "1.8.1",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.7",
    "cookie-session": "2.1.1",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "elastic-apm-node": "4.15.0",
    "express": "^4.22.1",
    "express-async-errors": "^3.1.1",
    "express-http-context": "2.0.1",
    "express-validator": "7.3.1",
    "helmet": "8.1.0",
    "http-status": "2.1.0",
    "inversify-inject-decorators": "3.1.0",
    "json-as-xlsx": "2.5.6",
    "jsonwebtoken": "9.0.3",
    "moment": "2.30.1",
    "mongoose": "^9.1.1",
    "morgan": "1.10.1",
    "nunjucks": "3.2.4",
    "reflect-metadata": "0.2.2",
    "socket.io": "^4.8.3",
    "swagger-ui-express": "5.0.1",
    "tsoa": "^7.0.0-alpha.0",
    "tsyringe": "^4.10.0",
    "uuid": "^8.3.2",
    "winston": "3.19.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@jest/types": "^30.2.0",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.19",
    "@types/errorhandler": "^1.5.3",
    "@types/express": "^5.0.6",
    "@types/glob": "^9.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/nunjucks": "^3.2.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "axios-mock-adapter": "^2.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
